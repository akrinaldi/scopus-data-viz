			  .data(function (row) {
			    return columns.map(function (column) {
			      return {column: column, value: row[column]};
			    });
			  })
			  .enter()
			  .append('td')
			    .text(function (d) { return d.value; });
			

		  return table;
		}

	
		// render the table(s)
		tabulate(morefill, ["auid","eid","title","publication","count"]); // 2 column table
		
});

</script>
